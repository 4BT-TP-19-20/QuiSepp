<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="QiSepp.LehrerSelectionController">
   <children>
      <AnchorPane fx:id="anchorPane" prefHeight="500.0" prefWidth="600.0" styleClass="anchorPaneControlls" stylesheets="@LehrerSelectionStyleSheet.css">
         <children>
            <Button layoutX="45.0" layoutY="205.0" mnemonicParsing="false" onAction="#btnNeuesQuiz" prefHeight="90.0" prefWidth="150.0" text="Neues Quiz&#10;">
               <font>
                  <Font size="16.0" />
               </font></Button>
            <Button layoutX="225.0" layoutY="205.0" mnemonicParsing="false" onAction="#btnAlleQuiz" prefHeight="90.0" prefWidth="150.0" text="Quiz bearbeiten&#10;">
               <font>
                  <Font size="17.0" />
               </font></Button>
            <Button layoutX="405.0" layoutY="205.0" mnemonicParsing="false" onAction="#btnErhalteneQuiz" prefHeight="90.0" prefWidth="150.0" text="Quiz von Schülern">
               <font>
                  <Font size="16.0" />
               </font></Button>
            <Button layoutX="-2.0" layoutY="-1.0" mnemonicParsing="false" onAction="#goBack" prefHeight="60.0" prefWidth="120.0" text="Zurück">
               <font>
                  <Font size="16.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </children>
</StackPane>
